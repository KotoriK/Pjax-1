{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "#": ["./src"],
      "#*": ["./src/*"],
      "#tester": ["./test"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  }
}
